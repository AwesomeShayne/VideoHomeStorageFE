<Window x:Class="VideoHomeStorage.FE.StreamOutputWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VideoHomeStorageFE"
        mc:Ignorable="d"
        Title="StreamOutputWindow" Height="500" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="5"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="5"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="5"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="5"/>
        </Grid.ColumnDefinitions>
        <Button Name="BeginStreamButton"
                Content="Start Stream"
                Grid.Row="2"
                Grid.Column="2"
                Height="25"/>
        <Image Name="StreamBox"
               Margin="5"
               Grid.Row="1"
               Grid.Column="1"
               Grid.ColumnSpan="3"/>
    </Grid>
</Window>
